  <process id="CheckExitOperationCarsProcess" name="月即将退出营运车辆确认流程">
    <extensionElements>
      <activiti:executionListener event="start" delegateExpression="${processLogListener}"></activiti:executionListener>
      <activiti:executionListener event="end" delegateExpression="${processLogListener}"></activiti:executionListener>
    </extensionElements>
    <startEvent id="startevent1" name="Start" activiti:initiator="initiator"></startEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="gatherCars" name="汇总月即将退出营运车辆" activiti:assignee="${initiator}" activiti:formKey="fm:file:bc-business-workflow/jiaoche/GatherCars.jsp">
      <documentation>确认即将退出营运的车辆</documentation>
      <extensionElements>
        <activiti:taskListener event="create" delegateExpression="${taskLogListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="verifyDate" name="确认交车日期" activiti:formKey="fm:cn/bc/business/workflow/jiaoche/VerifyDate.form">
      <extensionElements>
        <activiti:taskListener event="create" delegateExpression="${taskLogListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${assign2UnitContractHandlerListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="lastVerify" name="确认并发起交车或续保处理" activiti:assignee="${initiator}" activiti:formKey="fm:cn/bc/business/workflow/jiaoche/LastVerify.form">
      <extensionElements>
        <activiti:taskListener event="create" delegateExpression="${taskLogListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <serviceTask id="autoCarsExitOperation" name="自动发起车辆交车处理流程" activiti:delegateExpression="${serviceTask4BatchStartCarExitOperationProcess}">
      <extensionElements>
        <activiti:executionListener event="start" delegateExpression="${excutionLogListener}"></activiti:executionListener>
      </extensionElements>
    </serviceTask>
    <serviceTask id="autoCarsRenew" name="自动发起车辆续保处理流程" activiti:delegateExpression="${serviceTask4BatchStartCarRenewProcess}">
      <extensionElements>
        <activiti:executionListener event="start" delegateExpression="${excutionLogListener}"></activiti:executionListener>
      </extensionElements>
    </serviceTask>
    <inclusiveGateway id="inclusivegateway1" name="Inclusive Gateway" default="manual"></inclusiveGateway>
    <inclusiveGateway id="inclusivegateway2" name="Inclusive Gateway"></inclusiveGateway>
    <userTask id="managerVerify" name="分公司经理确认" activiti:formKey="fm:cn/bc/business/workflow/jiaoche/ManagerVerify.form">
      <extensionElements>
        <activiti:taskListener event="create" delegateExpression="${assign2UnitManagerListener}"></activiti:taskListener>
        <activiti:taskListener event="create" delegateExpression="${taskLogListener}"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow1" name="月即将退出营运车辆确认流程" sourceRef="startevent1" targetRef="gatherCars"></sequenceFlow>
    <sequenceFlow id="flow2" name="" sourceRef="gatherCars" targetRef="verifyDate"></sequenceFlow>
    <sequenceFlow id="flow4" name="" sourceRef="lastVerify" targetRef="inclusivegateway1"></sequenceFlow>
    <sequenceFlow id="exitOperation" name="交车" sourceRef="inclusivegateway1" targetRef="autoCarsExitOperation">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${autoStartCarExitOperationProcess}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="renew" name="续保" sourceRef="inclusivegateway1" targetRef="autoCarsRenew">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${autoStartCarRenewProcess}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" name="" sourceRef="autoCarsExitOperation" targetRef="inclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow9" name="" sourceRef="autoCarsRenew" targetRef="inclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow10" name="" sourceRef="inclusivegateway2" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="manual" name="手动处理" sourceRef="inclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${manual}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="" sourceRef="verifyDate" targetRef="managerVerify"></sequenceFlow>
    <sequenceFlow id="agreeFlow" name="确认" sourceRef="exclusivegateway1" targetRef="lastVerify"></sequenceFlow>
    <sequenceFlow id="flow13" name="" sourceRef="managerVerify" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="backFlow" name="退回重新确认" sourceRef="exclusivegateway1" targetRef="verifyDate">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${back}]]></conditionExpression>
    </sequenceFlow>
  </process>
